   91 A:f53e                                    ;
   92 A:f53e                                    ; on '*' return to monitor.
   93 A:f53e                                    ;
   94 A:f53e  c9 2a                              cmp #'*'
   95 A:f540  d0 03                              bne @procmnm
   96 A:f542  4c 47 ff                           jmp monz
   97 A:f545                                    ;
   98 A:f545                                    ; get the mnemonic from input line
   99 A:f545                                    ;
  100 A:f545                                    ;
  101 A:f545                                    ; get coded mnemonic into tmp
  102 A:f545                                    ; ((menm0 & 0x1f) << 10) + ((menm1 & 0x1f) << 5) + (menm2 & 0x1f)
  103 A:f545                                    ; so the opcode forms a word with 13 bits used.
  104 A:f545                                    ; impossible mnemonics produce a syntax error, invalid mnemonics will produce an
  105 A:f545                                    ; unknown opcode
  106 A:f545                                    ;
  107 A:f545                           @procmnm  
  107 A:f545  29 1f                              and #$1f             ; ex.:  PHP                                            'P' = $50 -> $10
  108 A:f547  85 6a                              sta tmph                ;
  109 A:f549  a9 00                              lda #$00
  110 A:f54b  8d 00 02                           sta in                ; put a zero into first position to mark input buffer old news.
  111 A:f54e  20 cd f4                           jsr nextch                ; $1F useful bits, have to make room for 5 more        `H' = $48, useful $08
  112 A:f551  0a                                 asl                    ; $3E                                                        $90
  113 A:f552  0a                                 asl                    ; $7C                                                        $20
  114 A:f553  0a                                 asl                    ; $F8                                                        $40
  115 A:f554  0a                                 asl                    ; $01F0 high bit-> carry                                     $80 + 0
  116 A:f555  26 6a                              rol tmph                ; $3FF0                                               $20    $80
  117 A:f557  0a                                 asl                    ; $03E0 high bit-> carry                                     $00 + 1
  118 A:f558  26 6a                              rol tmph                ; $7FE0                                               $41    $00
  119 A:f55a  85 69                              sta tmpl                ; $E0 , five lower bits free                                 $00
  120 A:f55c  20 cd f4                           jsr nextch                ; $1F                                                  'P' = $50
  121 A:f55f  29 1f                              and #$1f             ;                                                            $10
  122 A:f561  05 69                              ora tmpl                ;                                                            $10
  123 A:f563  85 69                              sta tmpl                ;                                                     $41    $10
  124 A:f565                                    ;
  125 A:f565                                    ; now we have the short form in tmp.
  126 A:f565                                    ; go on to find the mnemonic in opcodex table
  127 A:f565                                    ; and tehn return this index to the opcode lookup tables
  128 A:f565  a2 71                              ldx #(opcodez-opcodex-1)+2
  129 A:f567                           @firstfail 
  129 A:f567  ca                                 dex 
  130 A:f568                           @scndfail 
  130 A:f568  ca                                 dex 
  131 A:f569  10 03                              bpl @cont
  132 A:f56b  4c df f4                           jmp syntaxerr
  133 A:f56e                           @cont     
  133 A:f56e  bd 88 f5                           lda opcodex,x     high
  134 A:f571  c5 6a                              cmp tmph
  135 A:f573  d0 f2                              bne @firstfail
  136 A:f575  ca                                 dex 
  137 A:f576  bd 88 f5                           lda opcodex,x     low
  138 A:f579  c5 69                              cmp tmpl
  139 A:f57b  d0 eb                              bne @scndfail
  140 A:f57d  8a                                 txa 
  141 A:f57e  4a                                 lsr                    ; rest of the tables have bytes not words, so half it.
  142 A:f57f  85 60                              sta mnem
  143 A:f581  c8                                 iny                    ; point to char after mnemonic
  144 A:f582                                    ;
  145 A:f582                                    ; at this point, we have the address set in a1-a3 and the
  146 A:f582                                    ; mnemonic code number in mnem.
  147 A:f582                                    ; Now proceed to  evaluate address part.
  148 A:f582                                    ; Following modes are possible:
  149 A:f582                                    ; - None. Command does not take arguments, it is finished.
  150 A:f582                                    ; - Immediate:  #Value
  151 A:f582                                    ; - Adr/Rel:    Address
  152 A:f582                                    ; - Indexed:    Address,X|Y
  153 A:f582                                    ; - indirect:   (Address)
  154 A:f582                                    ; - indir. indxd (Address),Y
  155 A:f582                                    ; - indexed ind. (Address,X)
  156 A:f582  20 d3 f4                           jsr eatblank
  157 A:f585  4c c9 f6                           jmp eval_arg
