    1 A:f4cd                                    ;
    2 A:f4cd                                    ; Mini-Assembler
    3 A:f4cd                                    ;
    4 A:f4cd                                    ; Assembles single instruction at the program cursor into machine code.
    5 A:f4cd                                    ; Following the syntax documented in the Apple II Reference manual
    6 A:f4cd                                    ;
    7 A:f4cd                                    ; xa assembler seems to have some obscure problems with block markers, so everything
    8 A:f4cd                                    ; is on top level. I tried with block proc and (, but tht produced sntax errors far, far away
    9 A:f4cd                                    ; Solution would be to use a different assembler. but they all have their quirks.
   10 A:f4cd                                    ;
   11 A:f4cd                           nextch    
   11 A:f4cd  c8                                 iny 
   12 A:f4ce  b9 00 02                           lda in,y
   13 A:f4d1  60                                 rts 
   14 A:f4d2                                    ;
   15 A:f4d2                                    ; consume blanks and $ signs,
   16 A:f4d2                                    ; since all numbers are hex
   17 A:f4d2                                    ; returns first non ignored char in a
   18 A:f4d2                                    ;
   19 A:f4d2                           eatblp    
   19 A:f4d2  c8                                 iny 
   20 A:f4d3                           eatblank  
   20 A:f4d3  b9 00 02                           lda in,y
   21 A:f4d6  c9 20                              cmp #' '
   22 A:f4d8  f0 f8                              beq eatblp
   23 A:f4da  c9 24                              cmp #'$'
   24 A:f4dc  f0 f4                              beq eatblp
   25 A:f4de  60                                 rts 
   26 A:f4df                                    ; get code for menmonic
   27 A:f4df                                    ; input: a has first letter.
   28 A:f4df                                    ; return: mnem index in x
   29 A:f4df                           syntaxerr 
   29 A:f4df  20 15 ff                           jsr bell
   30 A:f4e2  a9 20                              lda #' '
   31 A:f4e4                           @blnks    
   31 A:f4e4  20 c2 fd                           jsr cout
   32 A:f4e7  88                                 dey 
   33 A:f4e8  10 fa                              bpl @blnks
   34 A:f4ea  a9 5e                              lda #'^'
   35 A:f4ec  20 c2 fd                           jsr cout
   36 A:f4ef  a6 49                              ldx spnt                ; restore entry point stack position
   37 A:f4f1  9a                                 txs 
   38 A:f4f2  4c 17 f5                           jmp asmz

   40 A:f4f5                           setaddr   
   40 A:f4f5  20 aa ff                           jsr zmode                ; clear (reused) monitor mode, scan idx
   41 A:f4f8  20 85 ff                           jsr getnum                ; leaves the eor'ed ':' in a and in,y pointing to next chat
   42 A:f4fb  60                                 rts 
   43 A:f4fc                           hasaddr   
   43 A:f4fc  a0 04                              ldy #$04             ; look at first 5 chars, beginning with last
   44 A:f4fe                           @hadrlp   
   44 A:f4fe  b9 00 02                           lda in,y
   45 A:f501  c9 3a                              cmp #':'
   46 A:f503  f0 f0                              beq setaddr                ; if so, set current address
   47 A:f505  88                                 dey 
   48 A:f506  10 f6                              bpl @hadrlp
   49 A:f508  a0 00                              ldy #$00             ; no address found, reset 'in' pointer
   50 A:f50a  60                                 rts 
   51 A:f50b                                    ;
   52 A:f50b                                    ; Mini-Assembler entry point.
   53 A:f50b                                    ;
   54 A:f50b                           asm_entry 
   54 A:f50b  d8                                 cld                    ; who knows where we came from... bcd mode tends to confuse hex based arithmetics.
   55 A:f50c                                    ;
   56 A:f50c                                    ; save stack pointer to spnt to return to clean state
   57 A:f50c                                    ; when processing syntax errors
   58 A:f50c                                    ; from deeper subroutine level.
   59 A:f50c                                    ; very crude exception handling.
   60 A:f50c                                    ; This makes it possible to handle syntax errors in subroutines
   61 A:f50c                                    ; without too much overhead - process is aborted and asm returns to prompt.
   62 A:f50c                                    ;
   63 A:f50c  ba                                 tsx 
   64 A:f50d  86 49                              stx spnt
   65 A:f50f  a5 3c                              lda a1l
   66 A:f511  85 3a                              sta pcl
   67 A:f513  a5 3d                              lda a1h
